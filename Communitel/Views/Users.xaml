<UserControl x:Class="Communitel.Views.Views.Users"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Communitel.Views.Views"
             mc:Ignorable="d" Width="409.399" Height="333.835">
    <DockPanel HorizontalAlignment="Left" Height="334" LastChildFill="False" VerticalAlignment="Top" Width="409">
        <StackPanel x:Name="view_users" Height="334" VerticalAlignment="Top" Width="100">
            <Label x:Name="label" Content="List users"/>
            <Button x:Name="button" Content="View 2 &gt;&gt;"/>
            <ListView x:Name="listView" Height="100">
                <ListView.View>
                    <GridView>
                        <GridViewColumn/>
                    </GridView>
                </ListView.View>
            </ListView>
        </StackPanel>
        <StackPanel x:Name="view_user" Height="334" VerticalAlignment="Top" Width="100">
            <Label x:Name="label1" Content="User"/>
            <Button x:Name="button1" Content="&lt;&lt; View 1"/>
        </StackPanel>
    </DockPanel>
</UserControl>
